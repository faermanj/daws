{
  "name": "DAWS Dev Container",
  "image": "mcr.microsoft.com/devcontainers/base",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {},
    "ghcr.io/audacioustux/devcontainers/graalvm:1": {},
    "ghcr.io/devcontainers/features/python:1": {},
    "ghcr.io/devcontainers/features/aws-cli:1": {},
    "ghcr.io/customink/codespaces-features/sam-cli:1": {},
    "ghcr.io/devcontainers-extra/features/aws-cdk:2": {},
    "ghcr.io/devcontainers-extra/features/amplify-cli:2": {},
    "ghcr.io/joshspicer/more-features/cowsay:1": {}
  },
  "postCreateCommand": "./scripts/postCreateCommand.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "aws-scripting-guy.cform",
        "vscjava.vscode-java-pack",
        "redhat.java",
        "ms-python.python"
      ]
    }
  }
}
